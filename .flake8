[flake8]

exclude=
    __pycache__,

    .gitignore,
    requirements.txt,

    documentation/,
    venv/,
    SoftDesk/users/migrations/,
    SoftDesk/users/tests.py,
    SoftDesk/projects/migrations/,
    SoftDesk/projects/migrations/tests.py,
    
    README.md,

max-line-length = 119
max-doc-length = 119

max-complexity = 10