[flake8]

exclude=
    __pycache__,

    .gitignore,
    requirements.txt,

    documentation/,
    venv/,
    accounts/migrations/,
    accounts/tests.py,
    projects/migrations/,
    projects/migrations/tests.py,
    
    README.md,

max-line-length = 119
max-doc-length = 119

max-complexity = 10